    "cypress": "^15.10.0",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.15.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.45.2",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@imgly/background-removal": "^1.7.0",
